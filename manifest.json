{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "SafeGuard",
  "description": "SafeGuard",
  "action": {
    "default_popup": "js/index.html",
    "default_title": "SafeGuard"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/scripts/contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "/scripts/background.js"
  }
}
